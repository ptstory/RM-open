<manifest>
    <main-class>com.ricespud.bots.waterbowls.WaterBowls</main-class>

    <name>WaterBowls</name>
    <tag-line>bowls</tag-line><!--Max of 50 chars-->
    <description>Fills bowls with water</description><!--Max of 110 chars-->
    <version>0.0.1</version>
    <compatibility>
        <game>OSRS</game>
    </compatibility>
    <categories>
        <category>MONEYMAKING</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>waterbowls</internal-id>
    <!--The rest are optional-->
    <open-source>false</open-source>
    <hidden>false</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <tags>
        <tag>Moneymaking</tag>
        <tag>Water</tag>
        <tag>F2P</tag>
    </tags>
    <resources>
        <resource>path/to/file.png</resource>
    </resources>
</manifest>
