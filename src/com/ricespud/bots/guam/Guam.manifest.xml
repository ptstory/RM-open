<manifest>
    <main-class>com.ricespud.bots.guam.Guam</main-class>

    <name>Guam</name>
    <tag-line>Guam</tag-line><!--Max of 50 chars-->
    <description>Makes the guam potion hehe</description><!--Max of 110 chars-->
    <version>0.0.1</version>
    <compatibility>
        <game>OSRS</game>
    </compatibility>
    <categories>
        <category>HERBLORE</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>AiO</internal-id>
    <!--The rest are optional-->
    <open-source>true</open-source>
    <hidden>false</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <tags>
        <tag>Tree</tag>
        <tag>Vine</tag>
    </tags>
    <resources>
        <resource>path/to/file.png</resource>
    </resources>
</manifest>
