    <!--The rest are optional-->
    <open-source>false</open-source>
    <hidden>false</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <tags>
        <tag>Balls</tag>
        <tag>Cannonballs</tag>
        <tag>AFK</tag>
        <tag>Money</tag>
    </tags>
    <resources>
        <!--<resource>src/com/ricespud/bots/cballs/ui/CballGUI.fxml</resource>-->
    </resources>
</manifest>
