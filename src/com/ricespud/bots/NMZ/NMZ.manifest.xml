<manifest>
    <main-class>com.ricespud.bots.NMZ.NMZ</main-class>

    <name>Rice NMZ</name>
    <tag-line>Forward to Edgeville!</tag-line><!--Max of 50 chars-->
    <description>Earn up to 168k gp/hr and 13k Smithing xp/hr! </description><!--Max of 110 chars-->
    <version>0.0.1</version>
    <compatibility>
        <game>OSRS</game>
    </compatibility>
    <categories>
        <category>COMBAT</category>
    </categories>
    <!--Required to publish on the bot store-->
